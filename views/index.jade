doctype html
html(lang="en")
  head
    title NoFeeDigs API
  body
    h1 There are #{total - badCount} good listings out of #{total} total listings.
    for problems, host in badHash
      h2= host
      for count, problem in problems
        if count > 0
          b( style='color: ' + (count > 100 ? 'red' : 'black') ) #{problem}: 
          = count
          br